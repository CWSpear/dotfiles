source ${ZDOTDIR:-$HOME}/.zprezto/runcoms/zpreztorc

# only things we need to override/add below here

zstyle ':prezto:load' pmodule \
  'environment' \
  'terminal' \
  'editor' \
  'history' \
  'directory' \
  'spectrum' \
  'utility' \
  'completion' \
  'fasd' \
  'syntax-highlighting' \
  'history-substring-search' \
  'osx' \
  'homebrew' \
  'git' \
  'prompt'

zstyle ':prezto:module:history-substring-search:color' found 'bg=blue,fg=white,bold'

zstyle ':prezto:module:prompt' theme 'sorinalt'
